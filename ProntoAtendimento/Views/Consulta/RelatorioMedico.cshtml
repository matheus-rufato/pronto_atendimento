@model List<Medico>

<br />

@if (@Model == null)
{
<h3>@ViewBag.Mensagem</h3> }
            else
            {

    <h5>Selecione o Médico: </h5>
                // <h3>@ViewBag.Mensagem @Model.Nome</h3>
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Nome</th>
        <th>Endereço</th>
        <th>Telefone</th>
        <th>Status</th>

    </tr>

    @foreach (var medic in Model)
    {
<tr>
    <td>@medic.Id</td>
    <td>@medic.Nome</td>
    <td>@medic.Endereco</td>
    <td>@medic.Telefone</td>
    <td>@medic.Status</td>

    <td>

        <a class="btn btn-sm btn-info" href="/consulta/relatorio/@medic.Id">Selecionar</a>

    </td>
</tr>}
</table>}
<a class="btn btn-primary" href="/consulta/relatoriopaciente">Voltar</a>
<a class="btn btn-danger" href="/home">Sair</a>