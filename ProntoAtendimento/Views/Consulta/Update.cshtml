@model Consulta

<form asp-controller="Consulta" asp-action="Update" method="POST">
    



    <div class="form-group">
        <label>Status</label>
        <select asp-for="Status" class="form-control">
            <option value="1">Ativa</option>
            <option value="2">Encerrada</option>
            <option value="3">Aberta</option>
        </select>
    </div>


    <div class="form-group">
        <label>Diagnsotico</label>
        <input asp-for="Diagnostico" class="form-control" />
        <span asp-validation-for="Diagnostico"></span>
    </div>



    <button class="btn btn-primary">Salvar</button>
    
</form>
<a class="btn btn-primary" href="/consulta/fim">Encerrar Consulta</a>
<a class="btn btn-primary" href="/procedimento/selection">Procedimentos</a>

