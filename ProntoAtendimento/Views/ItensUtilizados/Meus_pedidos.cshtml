@model List<ItensUtilizados>


<table class="table table-hover">
    <tr>
        <th>Confirmar Procedimentos?</th>


    </tr>

    <tr>
        <th>ID</th>
        <th>Nome</th>
        <th>Tipo</th>
        <th>Valor</th>

    </tr>


    @foreach (var item in Model)
    {
<tr>
    <td>@item.Procedimento.IdProcedimento</td>
    <td>@item.Procedimento.Nome</td>
    <td>@item.Procedimento.Tipo</td>
    <td>@item.Procedimento.Valor</td>


</tr>}




</table>


<a class="btn btn btn-primary" href="/itensutilizados/criar/">Confirmar</a>
<a class="btn btn-sm btn-danger" href="/listaprocedimentos/index/">Voltar</a>
