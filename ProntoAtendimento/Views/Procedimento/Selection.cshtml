@model List<Procedimento>

@{

    ViewData["Title"] = "Produto - Index";
}
<h5>Procedimentos</h5>

<table class="table table-hover">

    <tr>
        <th>ID</th>
        <th>Nome</th>
        <th>Tipo</th>
        <th>Valor</th>
        
    </tr>
    @foreach (var proced in Model)

    {
        <tr>
            <td>@proced.IdProcedimento</td>
            <td>@proced.Nome</td>
            <td>@proced.Tipo</td>
            <td>@proced.Valor</td>
            
            <td>
                
                <a class="btn btn-sm btn-info" href="/listaprocedimentos/escolher/@proced.IdProcedimento">Escolher</a>
            </td>

        </tr>
    }
</table>
<a class="btn btn-primary" href="/consulta/atender">Voltar</a>
